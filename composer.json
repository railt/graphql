{
    "name": "railt/sdl",
    "type": "library",
    "description": "The Railt GraphQL SDL is designed to build Reflection API classes from AST.",
    "license": "MIT",
    "keywords": [ "graphql", "idl", "sdl", "otd", "ast", "reflection", "reader", "parser", "llk", "introspection" ],
    "homepage": "http://railt.org",
    "support": {
        "issues": "https://github.com/railt/railt/issues",
        "source": "https://github.com/railt/railt"
    },
    "authors": [
        {
            "name": "Kirill Nesmeyanov",
            "email": "nesk@xakep.ru"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "railt/io": "~1.0",
        "railt/storage": "~1.0",
        "railt/compiler": "~1.0",
        "railt/reflection": "~1.0",
        "psr/log": "~1.0",
        "psr/cache": "~1.0",
        "psr/simple-cache": "~1.0",
        "illuminate/support": "~5.5",
        "zendframework/zend-code": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "Railt\\GraphQL\\": ""
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
