{
    "name": "railt/sdl",
    "license": "MIT",
    "type": "library",
    "homepage": "http://railt.org",
    "description": "The GraphQL RL/SDL grammar and compiler",
    "keywords": [
        "graphql",
        "idl",
        "sdl",
        "otd",
        "ast",
        "reflection",
        "compiler"
    ],
    "authors": [
        {
            "name": "Kirill Nesmeyanov",
            "email": "nesk@xakep.ru"
        }
    ],
    "support": {
        "issues": "https://github.com/railt/railt/issues",
        "source": "https://github.com/railt/sdl"
    },
    "require": {
        "php": "^7.1.3",
        "ext-mbstring": "*",
        "railt/io": "1.4.*|1.4.x-dev",
        "railt/storage": "1.4.*|1.4.x-dev",
        "railt/compiler": "1.4.*|1.4.x-dev",
        "railt/reflection": "1.4.*|1.4.x-dev",
        "illuminate/support": "~5.5"
    },
    "autoload": {
        "psr-4": {
            "Railt\\": "src/"
        },
        "files": [
            "./src/SDL/polyfill.php"
        ]
    },
    "require-dev": {
        "symfony/finder": "~4.0",
        "cache/filesystem-adapter": "~1.0",
        "phpunit/phpunit": "^6.5"
    },
    "autoload-dev": {
        "psr-4": {
            "Railt\\Tests\\SDL\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
