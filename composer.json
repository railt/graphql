{
    "name": "railt/sdl",
    "type": "library",
    "description": "The GraphQL RL/SDL grammar and compiler",
    "license": "MIT",
    "keywords": [
        "graphql",
        "idl",
        "sdl",
        "otd",
        "ast",
        "reflection",
        "compiler"
    ],
    "homepage": "http://railt.org",
    "support": {
        "issues": "https://github.com/railt/railt/issues",
        "source": "https://github.com/railt/sdl"
    },
    "authors": [
        {
            "name": "Kirill Nesmeyanov",
            "email": "nesk@xakep.ru"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "railt/io": "1.2.*|dev-master",
        "railt/storage": "1.2.*|dev-master",
        "railt/compiler": "1.2.*|dev-master",
        "railt/reflection": "1.2.*|dev-master",
        "illuminate/support": "~5.5"
    },
    "autoload": {
        "psr-4": {
            "Railt\\SDL\\": "src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0",
        "codedungeon/phpunit-result-printer": "~0.16"
    },
    "autoload-dev": {
        "psr-4": {
            "Railt\\Tests\\SDL\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
