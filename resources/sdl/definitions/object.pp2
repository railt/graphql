
//
// Object definition
//

#ObjectDefinition
  : Description()?
    ObjectDefinitionHead()
    ObjectDefinitionBody()?
  ;

#ObjectExtension
  : Description()?
    ::T_EXTEND:: ObjectDefinition()
  ;

ObjectDefinitionHead
  : ::T_TYPE:: TypeDefinitionHeader()
    TypeDefinitionImplements()?
    Directives()?
  ;

ObjectDefinitionBody
  : ::T_BRACE_OPEN::
      FieldDefinition()*
    ::T_BRACE_CLOSE::
  ;
